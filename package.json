{
  "name": "sveld",
  "version": "0.8.3",
  "license": "Apache-2.0",
  "description": "Generate TypeScript definitions for your Svelte components.",
  "author": "IBM",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "test": "run-p test:*",
    "test:unit": "tsnd node_modules/tape/bin/tape tests/*.test.ts",
    "test:integration": "node tests/integration",
    "format": "prettier --write '{src,tests}/**/*.ts'",
    "prepack": "run-s build test"
  },
  "peerDependencies": {
    "svelte": "^3.31.0"
  },
  "dependencies": {
    "@rollup/plugin-node-resolve": "^11.0.1",
    "acorn": "^8.0.4",
    "comment-parser": "^0.7.6",
    "fast-glob": "^3.2.5",
    "fs-extra": "^9.0.1",
    "prettier": "^2.2.1",
    "rollup": "^2.36.0",
    "rollup-plugin-svelte": "^7.0.0",
    "svelte": "^3.31.2",
    "svelte-preprocess": "^4.7.3",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.6",
    "@types/node": "^14.14.20",
    "@types/prettier": "^2.1.6",
    "@types/tape": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "tape": "^5.1.1",
    "ts-node-dev": "^1.1.1"
  },
  "bin": {
    "sveld": "./cli.js"
  },
  "files": [
    "lib",
    "cli.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ibm/sveld.git"
  },
  "homepage": "https://github.com/ibm/sveld",
  "bugs": "https://github.com/ibm/sveld/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "documentation",
    "docgen",
    "typescript",
    "definitions",
    "JSDocs"
  ],
  "prettier": {
    "printWidth": 120
  },
  "contributors": [
    "Eric Liu (https://github.com/metonym)"
  ]
}
